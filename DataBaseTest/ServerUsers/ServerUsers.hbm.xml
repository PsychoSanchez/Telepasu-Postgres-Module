<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
	namespace="NHibernateBasics" assembly="NHibernateBasics">
  <class name="ServerUsers" table="ServerUsers">
    <id name="ID" column="ID">
      <generator class="identity"/>
    </id>
    <property name="Name" column="Name" />
    <property name="LastName" column="LastName" />
    <property name="FirstName" column="FirstName" />
    <property name="StatusID" column="StatusID" not-null="true"  />
    <property name="NumberID" column="NumberID" not-null="true"  />
    <property name="Password" column="Password" not-null="true" />
    <property name="RightsID" column="RightsID" not-null="true"  />
  </class>
</hibernate-mapping>
