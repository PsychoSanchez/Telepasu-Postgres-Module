<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
	namespace="NHibernateBasics" assembly="NHibernateBasics">
  <class name="Users" table="Users">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="NumberID" column="NumberID" not-null="true" />
    <property name="UserInfoID" column="UserInfoID" not-null="true" />
    <property name="ServerUserID" column="ServerUserID" />
    <property name="NumberTypeID" column="NumberTypeID" />
  </class>
</hibernate-mapping>
